	"repository": {
		"comments": {
      "patterns": [
        {
          "match": "^\\s*\\*.*",
          "name": "comment.gibiane"
        }
      ]
    },
		"strings": {
      "patterns": [
        {
          "name": "string.quoted.gibiane",
          "begin": "'",
          "end": "'|\\n"
        }
      ]
    },
		"control": {
			"patterns":[
				{
					"match": "(?i)\\b(?:SINON|SINO|SI|FINSI|FINS|FIN|REPE|REPETER|QUIT|ITER|ITERER|FIN)\\b",
					"name": "keyword.control.statement.gibiane"
				}
			]
