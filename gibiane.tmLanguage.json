			"patterns":[
				{
					"match": "(?i)\\b(?:VRAI|FAUX|ET|OU|NON|EGA|NEG)\\b",
					"name": "keyword.gibiane"
				}
			]
		},
		"numbers": {
			"patterns":[
				{
					"match": "(?i)(?![\\w\\.])\\s*[+-]?\\.\\d+E?(?:[+-]?\\d*)",
					"name": "constant.numeric.gibiane"
				},
				{
					"match": "(?i)(?![\\w\\.])\\s*[+-]?\\d+\\.?\\d*E?(?:[+-]?\\d*)",
					"name": "constant.numeric.gibiane"
				}
			]
		},
