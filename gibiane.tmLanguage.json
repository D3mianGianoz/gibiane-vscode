{
  "name": "Gibiane",
	"scopeName": "source.gibiane",
  "patterns": [
		{
      "include": "#comments"
    },
		{
      "include": "#strings"
    },
		{
			"include":"#numbers"
		},
		{
			"include":"#types"
		},
		{
			"include":"#elements"
		},
		{
			"include":"#control"
		},
		{
			"include":"#keywords"
		},
		{
			"include": "#operators"
		},
		{
			"include": "#functions"
		}
