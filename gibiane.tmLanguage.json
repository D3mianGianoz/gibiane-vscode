{
  "name": "Gibiane",
  "scopeName": "source.gibiane",
  "patterns": [
		{
      		"include": "#comments"
    	},
		{
      		"include": "#strings"
    	},
		{
			"include": "#constants"
		},
		{
			"include": "#control"
		},
		{
			"include": "#keywords"
		},
		{
			"include": "#procedur"
		},
		{
			"include": "#functions"
		},
		{
			"include": "#types"
		},
		{
			"include": "#elements"
		}
	],
	"repository": {
		"comments": {
      "patterns": [
        {
          "match": "^\\*.*", 
          "name": "comment.gibiane"
        }
      ]
    },
		"strings": {
      "patterns": [
        {
          "name": "string.quoted.gibiane",
          "begin": "'",
          "end": "'|\\n"
        }
      ]
    },
		"control": {
			"patterns":[
				{
					"match": "(?i)\\b(?:SINON|SINO|SI|FINSI|FINS|FIN|REPE|REPETER|QUIT|QUITTER|ITER|ITERER|FIN|DEBP|DEBPROC|FINP|FINPROC)\\b",
					"name": "keyword.control.statement.gibiane"
				}
			]
		},
		"keywords": {
			"patterns":[
				{
					"match": "(?i)\\b(?:ET|OU|NON|EGA|NEG)\\b",
					"name": "keyword.boolean.gibiane"
				},
				{
					"match": "\\<EG|\\>EG",
					"name": "keyword.comparison.gibiane"
				},
				{
					"match": "\\<|\\>",
					"name": "keyword.comparison.gibiane"
				},
				{
					"match": "\\=|\\+|\\-|\\*|\\/|\\*\\*",
					"name": "keyword.arithmetic.gibiane"
				}
			]
		},
		"constants": {
			"patterns":[
				{
					"match": "(?i)\\b(?:VRAI|FAUX)\\b",
					"name": "constant.logical.gibiane"
				},
				{
					"match": "(?i)(?![\\w\\.])\\s*[+-]?\\.\\d+E?(?:[+-]?\\d*)",
					"name": "constant.numeric.gibiane"
				},
				{
					"match": "(?i)(?![\\w\\.])\\s*[+-]?\\d+\\.?\\d*E?(?:[+-]?\\d*)",
					"name": "constant.numeric.gibiane"
				}
			]
		},
		"types": {
			"patterns":[
				{
					"match": "(?i)\\b(?:ENTIER|FLOTTANT|LISTENTI|LISTREEL|LOGIQUE|LISTMOTS|TABLE|EVOLUTION|NUAGE|CHPOINT|CHAMELEM)\\b",
					"name": "support.type.gibiane"
				}
			]
		},
		"elements": {
			"patterns":[
				{
					"match": "(?i)\\b(?:POI1|SEG2|SEG3|TRI3|TRI4|TRI6|TRI7|QUA4|QUA5|QUA8|QUA9|RAC2|RAC3|CUB8|CU20|PRI6|PR15|LIA3|LIA4|LIA6|LIA8|MULT|TET4|TE10|PYR5|PY13|ATTA|SUPE|RAP3|LIP6|LIP8|POLY|CU27|PR21|TE15|PY19|SEG4|QU16|TR12|PR18|SEG6|TR21|QU36|C216|P126|TE56|PY91|SURE)\\b",
