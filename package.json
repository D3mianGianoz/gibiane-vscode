{
  "name": "gibiane-vscode",
  "displayName": "Gibiane",
  "description": "Gibiane syntax highlighting",
  "version": "0.0.1",
  "publisher": "CharlesZablit",
	"icon": "icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/charles-zablit/gibiane-vscode.git"
  },
  "engines": {
    "vscode": "^1.14.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "gibiane",
        "aliases": [
          "Gibiane"
        ],
        "extensions": [
          ".dgibi"
        ],
        "configuration": "./GibianeConfiguration.json"
      }
    ],
    "grammars": [
      {
        "language": "gibiane",
        "scopeName": "source.gibiane",
        "path": "./gibiane.tmLanguage.json"
      }
    ]
  },
  "dependencies": {
    "@types/vscode": "^1.53.0",
    "vscode-uri": "^3.0.2"
  },
  "devDependencies": {
    "@types/node": "^8.0.14",
    "typescript": "^4.2.2"
  },
  "scripts": {
    "compile": "tsc -b tsconfig.json",
    "watch": "tsc -b -w"
  }
}
